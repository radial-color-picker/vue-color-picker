{
  "name": "@radial-color-picker/vue-color-picker",
  "version": "5.0.1",
  "description": "Radial Color Picker - Vue",
  "author": "Rosen Kanev <rkunev@gmail.com>",
  "scripts": {
    "start": "vite",
    "build": "node scripts/build.js",
    "test": "jest --watch",
    "test:ci": "jest -i --ci -b",
    "lint": "eslint --ext .js,vue src",
    "prepublishOnly": "yarn build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install"
  },
  "main": "dist/vue-color-picker.cjs.js",
  "module": "dist/vue-color-picker.es.js",
  "unpkg": "dist/vue-color-picker.umd.min.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "dependencies": {
    "@radial-color-picker/rotator": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@size-limit/file": "5.0.2",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/babel-preset-app": "4.3.1",
    "@vue/compiler-sfc": "3.4.15",
    "@vue/test-utils": "2.0.0-rc.11",
    "babel-jest": "26.6.3",
    "chalk": "4.1.2",
    "cliui": "7.0.4",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "7.15.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "size-limit": "5.0.2",
    "vite": "5.0.12",
    "vitepress": "1.0.0-rc.40",
    "vue": "3.4.15",
    "vue-jest": "5.0.0-alpha.10"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "contributors": [
    "Dennis Dierkes <dierkes@smartsquare.de>"
  ],
  "homepage": "https://radial-color-picker.github.io/vue-color-picker/",
  "keywords": [
    "vue",
    "radial color picker",
    "color picker",
    "color-picker",
    "vue-color-picker",
    "hue picker",
    "nuxt",
    "nuxt module"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "yarn eslint",
      "yarn prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/vue-color-picker.umd.min.js",
      "limit": "2.8 kB"
    },
    {
      "path": "./dist/vue-color-picker.min.css",
      "limit": "1.4 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/radial-color-picker/vue-color-picker.git"
  }
}
