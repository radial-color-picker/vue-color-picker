{
  "name": "@radial-color-picker/vue-color-picker",
  "version": "5.0.0-alpha.1",
  "description": "Radial Color Picker - Vue",
  "author": "Rosen Kanev <rkunev@gmail.com>",
  "scripts": {
    "start": "vite",
    "build": "node scripts/build.js",
    "test": "jest --watch",
    "test:ci": "jest -i --ci -b",
    "lint": "eslint --ext .js,vue src",
    "prepublishOnly": "yarn build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "prepare": "husky install"
  },
  "main": "dist/vue-color-picker.cjs.js",
  "module": "dist/vue-color-picker.es.js",
  "unpkg": "dist/vue-color-picker.umd.min.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "@radial-color-picker/color-wheel": "2.2.2",
    "@radial-color-picker/rotator": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@size-limit/file": "5.0.2",
    "@vitejs/plugin-vue": "1.2.5",
    "@vue/babel-preset-app": "4.3.1",
    "@vue/compiler-sfc": "3.1.5",
    "@vue/component-compiler-utils": "3.2.2",
    "@vue/test-utils": "2.0.0-rc.11",
    "@vuepress/plugin-register-components": "1.8.2",
    "autoprefixer": "10.3.1",
    "babel-jest": "26.6.3",
    "chalk": "4.1.1",
    "cliui": "6.0.0",
    "eslint": "7.31.0",
    "eslint-plugin-vue": "7.14.0",
    "fs-extra": "9.0.0",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "lint-staged": "11.1.1",
    "postcss": "8.3.6",
    "postcss-loader": "6.1.1",
    "prettier": "2.0.5",
    "size-limit": "5.0.2",
    "vite": "2.4.3",
    "vue": "3.1.5",
    "vue-jest": "5.0.0-alpha.10",
    "vuepress": "2.0.0-beta.22"
  },
  "peerDependencies": {
    "vue": "^2.5.21"
  },
  "contributors": [
    "Dennis Dierkes <dierkes@smartsquare.de>"
  ],
  "homepage": "https://radial-color-picker.github.io/vue-color-picker/",
  "keywords": [
    "vue",
    "radial color picker",
    "color picker",
    "color-picker",
    "vue-color-picker",
    "hue picker",
    "nuxt",
    "nuxt module"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "yarn eslint",
      "yarn prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/vue-color-picker.umd.min.js",
      "limit": "2.8 kB"
    },
    {
      "path": "./dist/vue-color-picker.min.css",
      "limit": "1.4 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/radial-color-picker/vue-color-picker.git"
  }
}
